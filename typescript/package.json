{
  "name": "@wirechunk/extension-toolkit",
  "version": "0.0.22",
  "description": "Wirechunk extension toolkit",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wirechunk/extension-toolkit.git"
  },
  "author": "Wirechunk, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wirechunk/extension-toolkit/issues"
  },
  "homepage": "https://github.com/wirechunk/extension-toolkit#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "build",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    "./server": {
      "types": "./build/server/main.d.ts",
      "import": "./build/server/main.js"
    },
    "./server/util": {
      "types": "./build/server/util.d.ts",
      "import": "./build/server/util.js"
    },
    "./web": {
      "types": "./build/web/main.d.ts",
      "import": "./build/web/main.js"
    }
  },
  "scripts": {
    "codegen": "npm run codegen:hooks && npm run lint && npm run format",
    "codegen:hooks": "node scripts/codegen.ts",
    "typecheck": "tsc --build",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "npm run build:clean && npm run build:output",
    "build:clean": "rm -rf build",
    "build:output": "tsc --build"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "dependencies": {
    "@fastify/type-provider-json-schema-to-ts": "^4.0.1",
    "@wirechunk/schemas": "0.0.21",
    "cookie": "^1.0.2",
    "fastify": "^5.2.1"
  }
}
