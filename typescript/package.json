{
  "name": "@wirechunk/extension-toolkit",
  "version": "0.0.22",
  "description": "Wirechunk extension toolkit",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wirechunk/extension-toolkit.git"
  },
  "author": "Wirechunk, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wirechunk/extension-toolkit/issues"
  },
  "homepage": "https://github.com/wirechunk/extension-toolkit#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "build",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    "./server": {
      "types": "./build/server/main.d.ts",
      "import": "./build/server/main.js"
    },
    "./server/util": {
      "types": "./build/server/util.d.ts",
      "import": "./build/server/util.js"
    },
    "./web/contexts": {
      "types": "./build/web/contexts.d.ts",
      "import": "./build/web/contexts.js"
    }
  },
  "scripts": {
    "codegen": "npm run codegen:hooks && npm run codegen:graphql && npm run lint && npm run format",
    "codegen:hooks": "node --experimental-transform-types scripts/codegen.ts",
    "codegen:graphql": "graphql-codegen",
    "typecheck": "tsc --build",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "npm run build:clean && npm run build:output",
    "build:clean": "rm -rf build",
    "build:output": "tsc --build"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^5.1.0",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.21.0"
  },
  "dependencies": {
    "@fastify/type-provider-json-schema-to-ts": "^4.0.1",
    "@wirechunk/schemas": "0.0.21",
    "cookie": "^1.0.2",
    "fastify": "^5.2.1"
  }
}
