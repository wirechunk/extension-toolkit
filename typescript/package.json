{
  "name": "@wirechunk/extension-toolkit",
  "version": "0.0.29",
  "description": "Wirechunk extension toolkit",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wirechunk/extension-toolkit.git"
  },
  "author": "Wirechunk, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wirechunk/extension-toolkit/issues"
  },
  "homepage": "https://github.com/wirechunk/extension-toolkit/blob/main/typescript/README.md",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "types": "./build/main.d.ts",
      "import": "./build/main.js"
    },
    "./util": {
      "types": "./build/util.d.ts",
      "import": "./build/util.js"
    }
  },
  "scripts": {
    "codegen": "npm run codegen:hooks && npm run lint && npm run format",
    "codegen:hooks": "node scripts/codegen.ts",
    "typecheck": "tsc --build",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "npm run build:clean && npm run build:output",
    "build:clean": "rm -rf build",
    "build:output": "tsc --project tsconfig.build.json",
    "test": "node --test"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@wirechunk/schemas": "0.0.30",
    "cookie": "^1.1.1",
    "fastify": "^5.6.2"
  }
}
